CREATE TABLE movies (
    prod_id INTEGER NOT NULL,
    prod_price NUMERIC(10, 2) NOT NULL,
    prod_descrip VARCHAR(100) NOT NULL
);

create unique index movies_x0
on movies
(prod_id)
;

